{
  "name": "sim-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --src src",
