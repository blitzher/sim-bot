    "blizzard.js": "^4.4.0",
    "discord.js": "^14.11.0",
    "discordx": "^11.7.6",
    "node-fetch": "^3.3.1",
    "node-html-to-image": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.1.4",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
